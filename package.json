{
  "name": "qna",
  "description": "Webapp for Q&A in the classroom",
  "author": "Team Acetabulum",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/strburst/distrowatch-scraper.git"
  },
  "scripts": {
    "build": "gulp build",
    "gulp": "gulp",
    "start": "gulp watch"
  },
  "browserslist": [
    "> 1%"
  ],
  "babel": {
    "presets": [
      [
        "env"
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "jquery": true
    },
    "globals": {
      "d3": true
    },
    "rules": {
      "no-mixed-operators": ["off"],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-unused-vars": [
        "warn",
        {
          "vars": "local",
          "args": "after-used"
        }
      ]
    }
  },
  "devDependencies": {
    "babel-preset-env": "^1.2.1",
    "del": "^2.2.2",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.2.1",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^3.0.1",
    "gulp-eslint-if-fixed": "^1.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.1.0"
  }
}
